#!/usr/bin/php
<?php

function github_repos()
{
	# http://stackoverflow.com/a/8713664/1478566
	# > You can use the github api for this. Hitting
	# > https://api.github.com/users/username/repos will
	# > list that user's public repositories.

	$curl = curl_init();
	curl_setopt($curl, CURLOPT_URL, 'https://api.github.com/users/vbarbarosh/repos');
	curl_setopt($curl, CURLOPT_RETURNTRANSFER, true);
	curl_setopt($curl, CURLOPT_USERAGENT, 'curl');
	$response = curl_exec($curl);
	$errno = curl_errno($curl);
	$code = curl_getinfo($curl, CURLINFO_HTTP_CODE);
	curl_close($curl);

	if ($errno == 0 && $code == 200) {
		return json_decode($response);
	}

	echo '$errno', PHP_EOL;
	var_dump($errno);
	echo PHP_EOL;

	echo '$code', PHP_EOL;
	var_dump($code);
	echo PHP_EOL;

	echo '$response', PHP_EOL;
	var_dump($response);
	echo PHP_EOL;

	exit(1);

	# $errno
	# int(0)
	#
	# $code
	# int(403)
	#
	# $response
	# string(107) "{"message":"Missing or invalid User Agent string. See http://developer.github.com/v3/#user-agent-required"}"

	# $errno
	# int(0)
	#
	# $code
	# int(200)
	#
	# $response
	# string(159165) "[
	#   {
	#     "id": 1907687,
